/* Кнопки swiper  */
.sw {
  &__next, &__prev {
    position: absolute;
    background: url(../img/swiper_next.svg) center/contain no-repeat;
    width: 70px;
    height: 70px;
    z-index: 10;
    outline: none;
    cursor: pointer;
    transition: all .3s ease-in-out;

    &:hover {
      transform: scale(0.9);
    }
  }
  &__next {
    transform: scale(-1);
    &:hover {
      transform: scale(-0.9);
    }
  }
}




/* Animation */
*[data-animate-on-scroll] {
  opacity: 0;
}
*[data-animate-on-scroll=animated] {
  opacity: 1;
  animation: wipe-enter 2s;
}

@keyframes wipe-enter {
  0% {
    opacity: 0;
    transform: translate(0, -20px);
  }
  100% {
    opacity: 1;
    transform: translate(0);
  }
}

@keyframes blink {
  0% {
    opacity: 0;
  } 
  90% {
    opacity: 1;
  }
  100% {
    opacity: 0;
  }
}

